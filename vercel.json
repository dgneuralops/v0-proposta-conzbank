{
  "version": 2,
  "buildCommand": "npm install && npm run build",
  "installCommand": "npm install --legacy-peer-deps",
  "framework": "nextjs",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/" }
  ],
  "env": {
    "NODE_VERSION": "18.x"
  },
  "build": {
    "env": {
      "NEXT_TELEMETRY_DISABLED": "1"
    }
  }
}
